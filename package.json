{
  "name": "webadmin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:client": "vite --port 3004",
    "dev:api": "next dev -p 3001",
    "build": "npm run build:api && npm run build:client",
    "build:api": "next build",
    "build:client": "vite build",
    "start": "concurrently \"npm run start:api\" \"npm run start:client\"",
    "start:client": "vite preview --port 3004",
    "start:api": "next start -p 3001",
    "lint": "next lint"
  },
  "dependencies": {
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@types/request-ip": "^0.0.41",
    "@vitejs/plugin-react": "^5.0.4",
    "add": "^2.0.6",
    "apexcharts": "^4.5.0",
    "axios": "^1.6.0",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "flatpickr": "^4.6.13",
    "jspdf": "^2.5.1",
    "jsvectormap": "^1.7.0",
    "liquid-glass": "^1.0.0",
    "lucide-react": "^0.263.1",
    "next": "15.1.6",
    "next-themes": "^0.4.4",
    "nextjs-toploader": "^3.7.15",
    "pdfjs-dist": "^3.4.120",
    "react": "19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "19.0.0",
    "react-router-dom": "^6.26.1",
    "request-ip": "^3.3.0",
    "tailwind-merge": "^2.6.0",
    "uilayouts": "^1.0.3",
    "uuid": "^13.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^7.6.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.16",
    "typescript": "^5",
    "vite": "^5.0.0"
  }
}